<p id="notice"><%= notice %></p>

<h1>Syncs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @syncs.each do |sync| %>
      <tr>
        <td><%= sync.user_id %></td>
        <td><%= link_to 'Show', sync %></td>
        <td><%= link_to 'Edit', edit_sync_path(sync) %></td>
        <td><%= link_to 'Destroy', sync, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sync', syncs_path, method: :post %>
